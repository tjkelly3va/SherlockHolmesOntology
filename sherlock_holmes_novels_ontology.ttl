    ex:ledBy :sherlockHolmes ;
    ex:assistedBy :johnWatson ;
    ex:prefLabel "The Sign of the Four Investigation" ;
    ex:description "Holmes's investigation into the murder of Bartholomew Sholto and the theft of the Agra treasure, beginning with Mary Morstan's consultation and culminating in the Thames chase and capture of Jonathan Small." ;
    ex:memberOfOntology ex:SherlockOntology ;
    dc:source :signOfTheFourDoc .

:watsonProposalToMary a ex:Event ;
    ex:occurredAt :221bBakerStreet ;
    ex:hasParticipant :johnWatson, :maryMorstan ;
    ex:prefLabel "Watson's Proposal to Mary Morstan" ;
