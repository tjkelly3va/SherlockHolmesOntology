    ex:hasSuspect :jonathanSmall ;
    ex:prefLabel "Murder of Achmet" ;
    ex:description "The merchant Achmet was murdered by Abdullah Khan at Agra Fort during the Indian Mutiny. He was lured inside the fort with the promise of safe passage and killed so the four conspirators could seize the Agra treasure." ;
    ex:memberOfOntology ex:SherlockOntology ;
    dc:source :signOfTheFourDoc .

:indianMutiny a ex:Event ;
    ex:prefLabel "The Indian Mutiny" ;
    skos:altLabel "The Sepoy Rebellion" ;
